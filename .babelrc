{
  "presets": [
    ["es2015", { "modules": false }]
  ],
  "plugins": [
    "syntax-dynamic-import",
    ["transform-runtime", {
      "regenerator": false,
      "polyfill": false
    }],
    "fast-async"
  ],
  "comments": false,
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
