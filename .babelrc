{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "chrome": 56,
        "firefox": 52,
        "ios": 10,
        "safari": 10,
        "edge": 14,
        "uglify": true
      },
      "exclude": ["transform-regenerator", "transform-async-to-generator"],
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    ["fast-async", {
      "useRuntimeModule": true
    }]
  ],
  "comments": false
}
